generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Desk {
  id         Int      @default(autoincrement()) @id
  title      String
  employeeId Int
  Employee   Employee @relation(fields: [employeeId], references: [id])
}

model Employee {
  id    Int     @default(autoincrement()) @id
  name  String?
  email String  @unique
  Desk  Desk[]
}
